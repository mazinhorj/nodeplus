<h1>Fala, SEQUELADO!</h1>
<h2>Cadastrados:</h2>
<div class="container">
    {{#each users}}
        <div class="item">
            <h2>{{this.nome}}</h2>
            <ul><li>{{this.profissao}}</li></ul>
            <div class="actions">
                <a href="/users/{{this.id}}">+ Detalhes</a>
            </div>
        </div>
    {{/each}}
</div>
